<div>
    {{#if hasOrders }}
        <ul>
            {{#each orders}}
            <li>
                <h1>{{ this.user.name }}</h1>
                <ul>
                    {{#each this.items}}
                    <li>{{ this.title }} ({{ this.quantity}}) </li>
                    {{/each}}
                </ul>
            </li>
            {{/each}}
        </ul>
    {{else}}
        <h1>No Orders</h1>
    {{/if}}
</div>